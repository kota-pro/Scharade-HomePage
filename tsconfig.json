{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*",
    "src/**/*",
    "src/types/**/*"
  ],
  "exclude": [
    "dist",
    "next-app"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "typeRoots": ["./node_modules/@types", "./src/types"]
  }
}
